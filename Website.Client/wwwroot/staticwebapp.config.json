{
  "routes": [
    {
      "route": "/api/health",
      "allowedRoles": ["anonymous"],
      "methods": ["GET"]
    },
    {
      "route": "/api/blog/*",
      "allowedRoles": ["anonymous"],
      "methods": ["GET"]
    },
    {
      "route": "/api/blog/published",
      "allowedRoles": ["anonymous"],
      "methods": ["GET"]
    },
    {
      "route": "/api/blog/unpublished",
      "allowedRoles": ["authenticated"],
      "methods": ["GET"]
    },
    {
      "route": "api/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "redirect": "/notfound"
    }
  }
}