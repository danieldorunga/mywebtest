{
  "routes": [
    {
      "route": "api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "api/admin/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "redirect": "/notfound"
    }
  }
}